{
    "name": "New Tab Page for Salesforce",
    "description": "Search and access Salesforce and Chatter resources from your New Tab Page!",
    "version": "0.1.0",
    "background": {
        "scripts": ["./build/background.js"]
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "https://*.salesforce.com/",
        "cookies",
        "identity",
        "topSites",
        "storage"
    ],
    "manifest_version": 2
}
